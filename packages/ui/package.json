{
  "name": "@linknest/ui",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.tsx",
    "./index.css": "./dist/index.css"
  },
  "style": "./dist/index.css",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "postcss ./src/index.css -o ./dist/index.css --watch",
    "build": "postcss ./src/index.css -o ./dist/index.css",
    "lint": "eslint . --max-warnings 0",
    "generate:component": "turbo gen react-component",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@linknest/eslint-config": "workspace:*",
    "@linknest/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "eslint": "^9.31.0",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "@linknest/utils": "workspace:^",
    "@popperjs/core": "^2.11.8",
    "@tailwindcss/postcss": "^4.1.13",
    "daisyui": "^5.5.5",
    "lucide-react": "^0.543.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-draggable": "^4.5.0",
    "react-hook-form": "^7.66.1",
    "tailwindcss": "^4.1.13",
    "vaul": "^1.1.2"
  }
}
